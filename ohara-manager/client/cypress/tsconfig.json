{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es6",
    "module": "esnext",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "jsx": "react",
    "allowJs": true,
    "lib": ["es6", "dom", "es2017"],
    "types": ["cypress", "@types/testing-library__cypress"]
  }
}
