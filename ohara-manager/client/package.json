{
  "name": "ohara-manager-client",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api/*": {
      "target": "http://localhost:5050"
    }
  },
  "scripts": {
    "setup": "yarn && yarn build",
    "start": "react-scripts start",
    "start:ci": "forever start node_modules/react-scripts/scripts/start.js",
    "build": "react-scripts build",
    "lint": "node_modules/eslint/bin/eslint.js src cypress",
    "test": "react-scripts test --env=jsdom",
    "test:report": "JEST_JUNIT_OUTPUT=\"../testReports/clientUnits.xml\" CI=true yarn test --reporters=default --reporters=jest-junit",
    "test:coverage": "yarn test --coverage",
    "cypress": "cypress open",
    "cypress:ci": "cypress run"
  },
  "dependencies": {
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "forever": "0.15.3",
    "lodash": "4.17.10",
    "react": "16.4.1",
    "react-document-title": "2.0.3",
    "react-dom": "16.4.1",
    "react-modal": "3.5.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-test-renderer": "16.4.1",
    "styled-components": "3.3.3",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "cypress": "3.0.2",
    "eslint-plugin-cypress": "2.0.1",
    "jest-junit": "5.1.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true
    },
    "rules": {
      "jsx-a11y/href-no-hash": "off",
      "no-console": "warn"
    }
  }
}
