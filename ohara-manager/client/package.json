{
  "name": "ohara-manager-client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^8.12.0"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:5050"
    }
  },
  "scripts": {
    "setup": "yarn && yarn build",
    "start": "react-scripts start",
    "start:ci": "forever start node_modules/react-scripts/scripts/start.js",
    "build": "react-scripts build",
    "lint": "$(npm bin)/eslint src cypress",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "yarn test",
    "test:report": "CI=true yarn test --reporters=default --reporters=jest-junit",
    "test:coverage": "yarn test --coverage",
    "e2e": "cypress open",
    "e2e:ci": "cypress run"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "d3v4": "4.2.2",
    "dagre-d3": "0.6.1",
    "final-form": "4.11.0",
    "react": "16.4.1",
    "react-content-loader": "3.1.2",
    "react-document-title": "2.0.3",
    "react-dom": "16.4.1",
    "react-final-form": "4.0.2",
    "react-modal": "3.5.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-tabs": "2.3.0",
    "react-tooltip": "3.9.2",
    "styled-components": "3.3.3",
    "toastr": "2.1.4",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "cypress": "3.1.4",
    "cypress-testing-library": "2.3.4",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.0",
    "eslint-plugin-cypress": "2.0.1",
    "jest-junit": "5.1.0",
    "react-test-renderer": "16.4.1"
  }
}
